{
  "name": "akrabat/slim3-skeleton",
  "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
  "license": "BSD-3-Clause",
  "type": "project",
  "keywords": [
    "slim-framework",
    "skeleton"
  ],
  "authors": [
    {
      "name": "Rob Allen",
      "email": "rob@akrabat.com",
      "homepage": "http://akrabat.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/src"
    }
  },
  "require": {
    "slim/slim": "^3.0",
    "slim/twig-view": "^2.0",
    "slim/flash": "^0.1.0",
    "monolog/monolog": "^1.13",
    "twig/extensions": "^1.5",
    "illuminate/database": "~5.1",
    "illuminate/events": "5.2.*",
    "gregwar/gnuplot": "^1.0",
    "vlucas/phpdotenv": "^5.4"
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
